      },
      "description": "Provides last(p90) of successfull task invocations per validator+chain+task",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "center",
            "cellOptions": {
              "mode": "basic",
              "type": "color-background"
            },
            "filterable": true,
            "inspect": false
          },
          "fieldMinMax": false,
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "transparent",
                  "index": 0,
                  "text": "N/A"
                }
              },
              "type": "value"
            },
            {
              "options": {
                "pattern": "^(?!-?\\d+$).*",
                "result": {
                  "color": "transparent",
                  "index": 1
                }
              },
              "type": "regex"
            }
          ],
          "max": 15,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "semi-dark-green",
                "value": 0.2
              },
              {
                "color": "dark-green",
                "value": 0.5
              },
              {
                "color": "orange",
                "value": 2
              },
              {
                "color": "light-red",
                "value": 3
              },
              {
                "color": "dark-red",
                "value": 5
              }
            ]
          },
          "unit": "dtdurations"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "task_id\\job"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 258
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 30,
        "w": 24,
        "x": 0,
        "y": 16
