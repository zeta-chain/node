        "showHeader": true,
        "sortBy": []
      },
      "pluginVersion": "12.0.0+security-01",
      "targets": [
        {
          "editorMode": "code",
          "expr": "label_join (\nhistogram_quantile(0.9,\n    sum by (job, task_group, task_name, le) (\n        increase(zetaclient_scheduler_task_duration_seconds_bucket{status=\"ok\"}[$__range])\n    )\n),\n  \"task_id\", \":\", \"task_group\", \"task_name\"\n)",
          "format": "table",
          "legendFormat": "__auto",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Validator / Task P90",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "task_group": true,
              "task_name": true
            },
            "includeByName": {},
            "indexByName": {},
            "renameByName": {
              "job": "",
              "task_id": ""
            }
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Value": {
                "aggregations": [
                  "last"
                ],
                "operation": "aggregate"
              },
              "job": {
                "aggregations": [],
                "operation": "groupby"
              },
              "task_id": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        },
        {
          "id": "groupingToMatrix",
          "options": {
            "columnField": "job",
            "emptyValue": "zero",
            "rowField": "task_id",
            "valueField": "Value (last)"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "task_id\\\\job",
            "renamePattern": "Task"
          }
        }
      ],
      "type": "table"
    },
